local player = game.Players.LocalPlayer
local Foin = TweenInfo.new(0.5)
local tween = game:GetService("TweenService")
local funipart = Instance.new("Part")
funipart.Parent = workspace
funipart.CFrame = CFrame.new(598.322083, 4.06644678, -95.3310699)
funipart.Name = "WorkSpot"
funipart.Anchored = true
funipart.CanCollide = false
local funipart2 = Instance.new("Part")
funipart2.Parent = workspace
funipart2.CFrame = CFrame.new(590.752075, 4.0664463, -88.0091248)
funipart2.Name = "CashierPart"
funipart2.Anchored = true
funipart2.CanCollide = false
local goal = {CFrame = workspace.WorkSpot.CFrame}
local goal2 = {CFrame = workspace.CashierPart.CFrame}
local goal3 = {CFrame = game:GetService("Workspace").Map.NPCs.DeliNPC.HumanoidRootPart.CFrame}
local GoToFries = tween:Create(player.Character.HumanoidRootPart, Foin, goal)
local GoToCashier = tween:Create(player.Character.HumanoidRootPart, Foin, goal2)

if getgenv().SecurityOn == true then
local GoDeli = tween:Create(player.Character.HumanoidRootPart, Foin, goal3)
GoDeli:Play()
wait(0.1)
fireproximityprompt(game:GetService("Workspace").Map.NPCs.DeliNPC.HumanoidRootPart.PromptAttachment.ProximityPrompt)
game.ReplicatedStorage["_network"].purchase:InvokeServer("deli", "Shiesty")
wait(0.3)
player.Character.Humanoid:EquipTool(player.Backpack.Shiesty)
wait(0.01)
workspace[player.Name].Shiesty.RemoteEvent:FireServer()
wait(3)
getgenv().AutoFarmOn = true
player.Character.Humanoid:UnequipTools(workspace[player.Name].Shiesty)
else
    getgenv().AutoFarmOn = true
end

while getgenv().AutoFarmOn == true do
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
    GoToFries:Play()
    task.wait(0.5)
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
    fireproximityprompt(game:GetService("Workspace").Map.Jobs.InAndOut.FrieWork.ProximityAttachment.ProximityPrompt)
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
    task.wait(6)
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
    GoToCashier:Play()
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
    task.wait(1)
    game.Players.LocalPlayer.Character.Humanoid.Sit = false
end
